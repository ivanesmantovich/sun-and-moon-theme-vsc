    // semanticTokenColors OVERRIDE tokenColors
    "semanticHighlighting": true,

    "semanticTokenColors": {
        "function": "#007AFF",
        "method": "#007AFF",
        "class": "#007AFF",
        "macro": "#007AFF",

        "variable": "#000000",
        "keyword": "#000000",
        "string": "#000000",
        "operator": "#000000",
        "number": "#000000",
        "enumMember": "#000000",
        "decorator": "#000000",
        "parameter": "#000000",
        "typeParameter": "#000000",
        "property": "#000000",
        "event": "#000000",
        "label": "#000000",

        "struct": "#FF8200",
        "interface": "#FF8200",
        "type": "#FF8200",
        "namespace": "#FF8200",
        "enum": "#FF8200",

        "comment": "#4a5560",

        "regexp": "#3C3E39"
    },

    "tokenColors": [
        // Variable
        {
            "scope": "variable",
            "settings": { "foreground": "#000000", "fontStyle": "" }
        },
        // Functions
        {
            "scope": "entity.name.function",
            "settings": { "foreground": "#007AFF", "fontStyle": "bold" }
        },
        {
            "scope": "support.function",
            "settings": { "foreground": "#007AFF", "fontStyle": "bold" }
        },
        // Method
        {
            "scope": "entity.name.method",
            "settings": { "foreground": "#007AFF", "fontStyle": "bold" }
        },
        {
            "scope": "entity.name.type",
            "settings": { "foreground": "#FF8200" }
        },
        {
            "scope": "entity.name.type",
            "settings": { "foreground": "#FF8200" }
        },
        {
            "scope": "support.type",
            "settings": { "foreground": "#FF8200" }
        },
        // HTML Tag
        {
            "scope": "entity.name.tag",
            "settings": { "foreground": "#007AFF", "fontStyle": "bold" }
        },
        // HTML Tag Attibute name
        {
            "scope": "entity.other.attribute-name",
            "settings": { "foreground": "#ff8200" }
        },
        {
            "scope": "meta.property-name.css",
            "settings": { "foreground": "#ff8200" }
        },
        // CSS Selectors (Classes, IDs)
        {
            "scope": "entity.other.attribute-name.class.css",
            "settings": { "foreground": "#000000", "fontStyle": "bold" }
        },
        {
            "scope": "entity.other.attribute-name.id.css",
            "settings": { "foreground": "#000000", "fontStyle": "bold" }
        },
        // function, const, let, ...
        {
            "scope": "storage.type",
            "settings": { "foreground": "#000000", "fontStyle": "italic" }
        },
        // Arrow
        {
            "scope": "storage.type.function.arrow",
            "settings": { "foreground": "#000000", "fontStyle": "bold" }
        },
        // return, for, break, ...
        {
            "scope": "keyword.control",
            "settings": { "foreground": "#000000", "fontStyle": "bold" }
        },
        // String
        {
            "scope": "string",
            "settings": { "foreground": "#3C3E39", "fontStyle": "italic" }
        },
        // Comment
        {
            "scope": "comment",
            "settings": { "foreground": "#5d6c79", "fontStyle": "italic" }
        }
    ]
